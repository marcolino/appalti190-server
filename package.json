{
  "name": "appalti190",
  "version": "2.0.4",
  "description": "Servizio per l'assolvimento degli obblighi di pubblicazione delle informazioni all’Autorità Nazionale AntiCorruzione, ai sensi della legge n. 190/2012",
  "engines": {
    "node": "14.x",
    "npm": "6.x",
    "yarn": "1.x"
  },
  "main": "server.js",
  "scripts": {
    "start-dev": "nodemon --ignore client/ -w '.' -w .env server.js",
    "start": "node server.js",
    "test": "mocha --exit --timeout 10000",
    "testwatch": "nodemon --exec \"mocha --recursive --timeout 10000\"",
    "pub": "git add . && git commit -m \"deploying to heroku\" && git push && yarn deploy",
    "pub-dev": "yarn pub && yarn logs",
    "deploy": "git push heroku master",
    "SUSPENDED-predeploy": "yarn test",
    "logs": "heroku logs --tail",
    "check-unused-dependencies": "depcheck",
    "list-deadfiles": "npx deadfile ./server.js --dir src",
    "refresh-client_DO_NOT_USE__CLIENT_WILL_COPY_HERE": "cp -a ../sistemi190-client/build client/"
  },
  "keywords": [
    "node.js",
    "express",
    "jwt",
    "refresh token",
    "authentication",
    "authorization",
    "mongodb"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-routes-versioning": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.10",
    "multer": "^1.4.4",
    "node-zip": "^1.1.1",
    "should": "^13.2.3",
    "stripe": "^8.214.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "xlsx": "^0.18.5",
    "xmlbuilder": "^15.1.1",
    "xsd-schema-validator": "^0.7.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15"
  }
}
